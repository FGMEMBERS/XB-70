    <object-name>ElevonsRightMovF</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Left wing tip</object-name>
    <property>surface-positions/wing-fold-pos-norm</property>
    <factor>-65</factor>
    <axis>
      <x1-m> 19.717 </x1-m>
      <y1-m> -9.442 </y1-m>
      <z1-m>  2.523 </z1-m>
      <x2-m> 11.240 </x2-m>
      <y2-m> -9.442 </y2-m>
      <z2-m>  2.075 </z2-m>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>Right wing tip</object-name>
    <property>surface-positions/wing-fold-pos-norm</property>
    <factor>65</factor>
    <axis>
      <x1-m> 19.717 </x1-m>
      <y1-m>  9.442 </y1-m>
