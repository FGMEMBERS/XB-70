<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- Altitude-Vertical Velocity Indicator -->

<PropertyList>

  <path>alt.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>face</object-name>
    <object-name>visse1</object-name>
    <object-name>visse2</object-name>
    <object-name>visse3</object-name>
    <object-name>visse4</object-name>
    <object-name>bouton</object-name>
    <object-name>indaltt</object-name>
    <object-name>indalth</object-name>
    <object-name>indalti</object-name>
    <object-name>indaltlmob</object-name>
    <object-name>barre</object-name>
    <object-name>alt10000</object-name>
    <object-name>alt1000</object-name>
    <object-name>alt100</object-name>
    <object-name>alt10</object-name>
    <object-name>alt1</object-name>
    <object-name>target1</object-name>
    <object-name>target2</object-name>
    <object-name>target3</object-name>
    <object-name>target4</object-name>
    <object-name>target5</object-name>
    <object-name>num1</object-name>
    <object-name>num2</object-name>
    <object-name>num3</object-name>

    
  </animation>

  <!--<animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>shader.png</texture>
    <object-name>vitre</object-name>
  </animation>-->

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>indaltt</object-name>
    <object-name>indalth</object-name>
    <object-name>indalti</object-name>
    <object-name>indaltlmob</object-name>
    <object-name>barre</object-name>
    <object-name>alt10000</object-name>
    <object-name>alt1000</object-name>
    <object-name>alt100</object-name>
    <object-name>alt10</object-name>
    <object-name>alt1</object-name>
    <object-name>target1</object-name>
    <object-name>target2</object-name>
    <object-name>target3</object-name>
    <object-name>target4</object-name>
    <object-name>target5</object-name>
    <object-name>num1</object-name>
    <object-name>num2</object-name>
    <object-name>num3</object-name>
    <emission>
      <red>   0.030 </red>
      <green> 0.015 </green>
      <blue>  0.005 </blue>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>textranslate</type>
    <object-name>indaltt</object-name>
    <property>instrumentation/altimeter/indicated-altitude-ft</property>
    <interpolation>
      <entry><ind>     0 </ind><dep> 0.00 </dep></entry>
      <entry><ind> 49000 </ind><dep> 0.96 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>indalth</object-name>
    <property>instrumentation/altimeter/indicated-altitude-ft</property>
    <factor>0.001</factor>
    <offset>0</offset>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center>
  </animation>
 
  <animation>
    <type>translate</type>
    <object-name>indaltlmob</object-name>
    <property>instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
    <interpolation>
      <entry><ind> -10000 </ind><dep> -0.078 </dep></entry>
      <entry><ind>  -1000 </ind><dep> -0.051 </dep></entry>
      <entry><ind>   -500 </ind><dep> -0.026 </dep></entry>
      <entry><ind>      0 </ind><dep>  0.000 </dep></entry>
      <entry><ind>    500 </ind><dep>  0.026 </dep></entry>
      <entry><ind>   1000 </ind><dep>  0.051 </dep></entry>
      <entry><ind>  10000 </ind><dep>  0.078 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
    <center>
      <x-m> 0 </x-m>
      <y-m> 0 </y-m>
      <z-m> 0 </z-m>
    </center>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>alt10000</object-name>
    <property>autopilot/settings/target-altitude-ft</property>
    <factor>0.00001</factor>
    <step>10000</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>alt1000</object-name>
    <property>autopilot/settings/target-altitude-ft</property>
    <factor>0.0001</factor>
    <step>1000</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>alt100</object-name>
    <property>autopilot/settings/target-altitude-ft</property>
    <factor>0.001</factor>
    <step>100</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>alt10</object-name>
    <property>autopilot/settings/target-altitude-ft</property>
    <factor>0.01</factor>
    <step>10</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
  <animation>
    <type>textranslate</type>
    <object-name>alt1</object-name>
    <property>autopilot/settings/target-altitude-ft</property>
    <factor>0.1</factor>
    <step>1</step>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
 
