<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <!-- Braking Chute -->
  <path>DragChutes.ac</path>

  <animation>
    <type>select</type>
    <object-name>DragChutes</object-name>
    <condition>
      <greater-than>
        <property>surface-positions/speedbrake-pos-norm</property>
	<value> 0.8 </value>
      </greater-than>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>DragChutesStrech</object-name>
    <condition>
      <greater-than>
        <property>surface-positions/speedbrake-pos-norm</property>
	<value> 0.4 </value>
      </greater-than>
    </condition>
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>DragChutesStrech</object-name>
    <condition>
      <less-than>
        <property>surface-positions/speedbrake-pos-norm</property>
	<value> 0.8 </value>
      </less-than>
    </condition>
  </animation>
  
 <animation>
    <type>scale</type>
    <object-name>DragChutesStrech</object-name>
    <property>surface-positions/speedbrake-pos-norm</property>
    <x-factor> 0.5 </x-factor>
    <x-offset> 1.0 </x-offset>
    <y-factor> 0.5 </y-factor>
    <y-offset> 1.0 </y-offset>
    <z-factor> 0.5 </z-factor>
    <z-offset> 1.0 </z-offset>
    <center>
      <x-m> 17.713 </x-m>
      <y-m>  0.000 </y-m>
      <z-m>  3.312 </z-m>
    </center>
 </animation>


</PropertyList>

