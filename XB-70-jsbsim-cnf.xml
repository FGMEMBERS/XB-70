<PropertyList include="XB-70-base.xml">

  <sim>

    <description>North American Valkyrie XB-70 (JSBSim)</description>
    <flight-model>jsb</flight-model>
    <aero>XB-70-jsbsim</aero>

 <systems>
  <autopilot>
   <path>Aircraft/XB-70/Systems/autopilot-jsbsim.xml</path>
  </autopilot>
 </systems> 


  </sim>

  <nasal>
    <XB70>
      <file>Aircraft/XB-70/Nasal/jsbsim.nas</file>
    </XB70>
  </nasal>
  
  <surface-positions>
  <wing-fold-pos-norm type="double">0</wing-fold-pos-norm>
  </surface-positions>
  
  <fdm><jsbsim><fcs>
  <wing-fold-cmd-norm type="double">0</wing-fold-cmd-norm>
  <wing-fold-pos-norm type="double">0</wing-fold-pos-norm>
  </fcs></jsbsim></fdm>
  
  <position><gear-agl-m type="double">0</gear-agl-m></position>
 
  <engines>
    <engine n="0">
   <running>true</running>
    </engine>
    <engine n="1">
   <running>true</running>
    </engine>
    <engine n="2">
   <running>true</running>
    </engine>
    <engine n="3">
   <running>true</running>
    </engine>
    <engine n="4">
   <running>true</running>
    </engine>
    <engine n="5">
   <running>true</running>
    </engine>
  </engines> 

</PropertyList>
