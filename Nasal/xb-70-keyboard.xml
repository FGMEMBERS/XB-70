<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

   <key n="68">
    <name>D</name>
    <desc>bay</desc>
    <binding>
     <command>nasal</command>
     <script>doors.doorsystem.passengerexport();</script>
    </binding>
   </key>

   <key n="70">
    <name>F</name>
    <desc>wing tips down</desc>
  <binding>
   <command>nasal</command>
   <script>controls.wingSweep(1)</script>
  </binding>
  <mod-up>
   <binding>
    <command>nasal</command>
    <script>controls.wingSweep(0)</script>
   </binding>
  </mod-up>
   </key>

   <key n="100">
    <name>d</name>
    <desc>door</desc>
    <binding>
     <command>nasal</command>
     <script>doors.doorsystem.crewexport();</script>
    </binding>
   </key>

   <key n="102">
    <name>f</name>
    <desc>wing tips up</desc>
  <binding>
   <command>nasal</command>
   <script>controls.wingSweep(-1)</script>
  </binding>
  <mod-up>
   <binding>
    <command>nasal</command>
    <script>controls.wingSweep(0)</script>
   </binding>
  </mod-up>
   </key>

   <key n="10">
    <name>Ctrl-j</name>
    <desc>Jump signal</desc>
    <binding>
     <command>property-assign</command>
     <property>controls/signal</property>
     <value>true</value>
    </binding>
  </key>

</PropertyList>

